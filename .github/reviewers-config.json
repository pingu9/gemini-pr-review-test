{
  "codeOwners": {
    "frontend/": ["binary-ho", "binary-ho"],
    "backend/api/": ["binary-ho", "binary-ho"],
    "backend/auth/": ["binary-ho"],
    "infrastructure/": ["binary-ho", "binary-ho"],
    "*.sql": ["binary-ho"],
    "package.json": ["binary-ho"]
  },
  "defaultReviewers": ["binary-ho"],
  "minReviewers": 1,
  "maxReviewers": 3,
  "timezone": {
    "enabled": false,
    "workingHours": {
      "start": 9,
      "end": 18
    },
    "userTimezones": {
      "user1": "Asia/Seoul",
      "user2": "America/New_York",
      "user3": "Europe/London"
    }
  },
  "excludeAuthors": false,
  "skipDraft": true
}
